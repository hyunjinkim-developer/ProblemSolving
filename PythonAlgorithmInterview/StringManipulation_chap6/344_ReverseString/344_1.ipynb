   },
   "outputs": [],
   "source": [
    "def reverseString(self, s: List[str]) -> None:\n",
    "    left, right = 0, len(s) -1\n",
    "    while left < right:\n",
    "        s[left], s[right] = s[right], s[left]\n",
    "        left += 1\n",
    "        right -= 1"
   ]
  }
